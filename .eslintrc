{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "plugins": ["json", "import"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-console": ["warn", {
      "allow": ["info", "warn", "error", "time", "timeEnd"]
    }]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
